{
  "name": "Watch and Learn",
  "version": "1.0",
  "icons": {
    "128": "static/icon128.png"
  },
  "description": "A chrome extension to make learn videos better using video.js",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://learn.uwaterloo.ca/*"
      ],
      "js": [
        "build/main.js"
      ],
      "css": [
        "static/video-js.css"
      ]
    }
  ]
}